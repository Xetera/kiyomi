{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "dev": "yarn run start --onSuccess 'node dist/index.js'",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "codegen": "cd .. && SHOULD_EXIT_AFTER_GENERATE_ARTIFACTS=true ts-node -P tsconfig.generator.json --transpile-only lib/schema.ts && cd labeler && graphql-codegen",
    "size": "size-limit",
    "tf": "npm rebuild @tensorflow/tfjs-node build-addon-from-source",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "name": "labeler",
  "private": "true",
  "author": "Xetera",
  "module": "dist/labeler.esm.js",
  "size-limit": [
    {
      "path": "dist/labeler.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/labeler.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/amqplib": "^0.5.17",
    "@types/convict": "^6.0.1",
    "@types/pino": "^6.3.7",
    "husky": "^6.0.0",
    "pino-pretty": "^4.7.1",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "1.7.4",
    "amqplib": "^0.7.1",
    "axios": "^0.21.1",
    "canvas": "^2.7.0",
    "convict": "^6.0.1",
    "dotenv": "^8.2.0",
    "face-api.js": "^0.22.2",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.11.0",
    "pino": "^6.11.2",
    "regenerator-runtime": "^0.13.7",
    "ts-node": "^9.1.1"
  }
}
